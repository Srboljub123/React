    "build": "rm -rf build && cross-env NODE_ENV=production webpack --mode production",
    "transpile": "rm -rf package && cross-env NODE_ENV=production npx babel --extensions \".ts,.tsx\" src --out-dir package --copy-files",
    "transpile:tsc": "npx -p typescript tsc package/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir @types",
    "cy:open": "npx cypress open",
    "cy:coverage": "npx nyc report --reporter=text-summary",
    "lint": "eslint --ext .tsx,.ts src",
    "lint:verify": "npx eslint --ext .tsx,.ts src",
    "lint:fix": "eslint --ext .tsx,.ts --fix src",
    "lint:css": "npx stylelint **/*.styles.ts",
    "nvm:use": "nvm use",
    "prepare": "husky install",
    "publish": "sudo docker build --tag react .",
    "publish:dev": "sudo docker build --tag react . && sudo docker run --publish 5010:5010 react",
    "start": "cross-env NODE_ENV=development webpack s --mode development",
    "test": "jest --runInBand --watchAll",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm lint",
      "pre-push": "npm test"
    }
